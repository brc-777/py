def safe(board, r, c):
    for i in range(r):
        if board[i] == c or abs(board[i] - c) == abs(i - r):
            return False
    return True

def solve(r, board):
    if r == 8:
        print(board)
        return True
    for c in range(8):
        if safe(board, r, c):
            board[r] = c
            if solve(r+1, board):
                return True

board = [-1]*8
solve(0, board)
